<h1>Registered versions</h1>

<table class="table table-striped">
	<thead>
		<tr>
			<th>#</th>
			<th>App Key</th>
			<th>Latest Version</th>
		</tr>
	</thead>
	<tbody>
		<% @versions.each_with_index do |version, index| %>
		<tr>
			<td><%= index+1 %></td>
			<td><%= version.app_key %></td>
			<td><%= version.latest_version %></td>
		</tr>
		<% end %>
	</tbody>
</table>
